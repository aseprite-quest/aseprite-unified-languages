        "path": "./ja.ini",
        "englishName": "Japanese",
        "displayName": "日本語"
      },
      {
        "id": "ko",
        "path": "./ko.ini",
        "englishName": "Korean",
        "displayName": "한국어"
      },
      {
        "id": "ru",
        "path": "./ru.ini",
        "englishName": "Russian",
        "displayName": "Русский"
      },
      {
        "id": "it",
        "path": "./it.ini",
        "englishName": "Italian",
        "displayName": "italiano"
