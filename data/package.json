{
  "name": "unified-languages",
  "displayName": "Unified Languages",
  "description": "Aseprite extension to support multiple languages.",
  "version": "1.3-rc8",
  "author": {
    "name": "Aseprite Quest",
    "url": "https://github.com/aseprite-quest"
  },
  "contributors": [
    {
      "name": "TakWolf",
